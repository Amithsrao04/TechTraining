#include <iostream>
using namespace std;
int removeDuplicates(int arr[], int n, int unique[]) {
int k = 0;
for (int i = 0; i < n; i++) 
{
int isDuplicate = 0;
for (int j = 0; j < k; j++) 
{
if (arr[i] == unique[j]) 
{
isDuplicate = 1;
break;
}
}
if (!isDuplicate) 
{
unique[k] = arr[i];
k++;
}
}
return k;
}
int main() 
{
int n;
cout << "Enter the number of elements in the array: ";
cin >> n;
int arr[n];
cout << "Enter " << n << " elements:\n";
for (int i = 0; i < n; i++) 
{
cin >> arr[i];
}
int unique[n];
int newSize = removeDuplicates(arr, n, unique);
cout << "Array after removing duplicates:\n";
for (int i = 0; i < newSize; i++) 
{
cout << unique[i] << " ";
}
return 0;
}

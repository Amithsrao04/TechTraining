#include<iostream>
using namespace std;
int main()
{
string s,s1;
int i,j,len=0;
std::getline(std::cin,s);
cout<<"Given String:"<<<s<<endl;
for(i=0;s[i]!='\0';i++)
{
len++;
}
cout<<"Length of s: "<<len<<endl;
cout<<"All the substring are: "<<endl;
for(i=0;i<len;i++)
{
for(j=0;j<=len;j++)
{
s1=s.substr(i,j);
cout<<s1<<endl;
}
}
return 0;	
}
